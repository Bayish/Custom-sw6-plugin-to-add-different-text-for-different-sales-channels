<?xml version="1.0" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
        <service id="BmAdvancedSnippets\Core\System\SnippetVariant\SnippetVariantDefinition">
            <tag name="shopware.entity.definition" entity="snippet_variant" />
        </service>
        <service id="BmAdvancedSnippets\Twig\CustomTwigExtension" class="BmAdvancedSnippets\Twig\CustomTwigExtension">
            <argument type="service" id="translator" />
            <argument type="service" id="snippet_variant.repository" />
            <argument type="service" id="sales_channel_domain.repository"/>
            <tag name="twig.extension" />
        </service>
    </services>
</container>
